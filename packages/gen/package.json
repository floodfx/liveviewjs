{
  "name": "@liveviewjs/gen",
  "version": "0.10.1",
  "description": "Code generators for LiveViewJS",
  "homepage": "https://liveviewjs.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/floodfx/liveviewjs",
    "directory": "packages/gen"
  },
  "bugs": {
    "url": "https://github.com/floodfx/liveviewjs/issues"
  },
  "type": "module",
  "main": "./dist/cli.mjs",
  "bin": "./dist/cli.mjs",
  "types": "./dist/cli.d.mts",
  "files": [
    "dist/**/*",
    "_templates/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dist": "npm run clean; npm run build",
    "prepublish": "npm run dist",
    "format": "prettier --write '**/*.{ts,js,json,html,css}'"
  },
  "keywords": [
    "liveviewjs",
    "liveview",
    "phoenix",
    "typescript",
    "javascript",
    "framework"
  ],
  "dependencies": {
    "chalk": "^5.2.0",
    "cli-spinners": "^2.7.0",
    "enquirer": "^2.3.6",
    "execa": "^6.1.0",
    "fs-extra": "^11.1.0",
    "hygen": "^6.2.11",
    "log-update": "^5.0.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/execa": "^2.0.0",
    "@types/figlet": "^1.5.5",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "rollup": "^2.70.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
