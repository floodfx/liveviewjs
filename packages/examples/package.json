{
  "name": "liveviewjs-examples",
  "version": "0.0.1",
  "description": "Example LiveViews showing how LiveViewJS used by Express and Deno server examples",
  "exports": {
    "require": "./dist/liveview-examples.js",
    "import": "./dist/liveview-examples.mjs"
  },
  "main": "dist/liveview-examples.js",
  "types": "dist/liveview-examples.d.ts",
  "files": [
    "dist/liveview-examples.js",
    "dist/liveview-examples.d.ts",
    "dist/liveview-examples.mjs"
  ],
  "scripts": {
    "rollup": "rollup -c",
    "copy-build-to-dist": "mkdir -p dist; cp build/liveview-examples.* dist",
    "dist": "npm run clean; npm run rollup; npm run copy-build-to-dist",
    "prepublish": "npm run dist",
    "build": "tsc",
    "clean": "rm -rf build"
  },
  "keywords": [
    "liveviewjs",
    "liveview",
    "phoenix",
    "typescript",
    "javascript"
  ],
  "author": "Donnie Flood <donnie@floodfx.com>",
  "license": "MIT",
  "dependencies": {
    "liveviewjs": "file:../../liveviewjs-0.3.0-rc.1.tgz",
    "nanoid": "^3.2.0",
    "node-fetch": "^2.6.7",
    "zod": "^3.14.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.0",
    "nodemon": "^2.0.15",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
